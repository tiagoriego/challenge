{
  "name": "challenge",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepare": "husky",
    "start:dev": "NODE_PATH=. nest start --watch",
    "db:create_migration": "NODE_PATH=. typeorm migration:create src/database/migration/$npm_config_name",
    "db:migrate": "NODE_ENV=development NODE_PATH=. npm run build && typeorm migration:run -d dist/database/data-source.database.js",
    "db:seed": "NODE_PATH=. ts-node test/seed.ts",
    "db:drop": "NODE_ENV=development NODE_PATH=. npm run build && typeorm schema:drop -d dist/database/data-source.database.js",
    "lint": "eslint .",
    "test": "NODE_PATH=. jest",
    "test:e2e": "NODE_PATH=. jest --config ./jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/apollo": "^13.0.2",
    "@nestjs/common": "^11.0.6",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.6",
    "@nestjs/graphql": "^13.0.2",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/typeorm": "^11.0.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.5",
    "@testdeck/jest": "^0.3.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
